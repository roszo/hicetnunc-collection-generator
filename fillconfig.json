{
  "cloudFlareUrl": "https://cloudflare-ipfs.com/ipfs/",
  "downloadPath": "./tmp/downloads",
  "largeImagePath": "./tmp/large-images",
  "distPath": "./tmp",
  "fillMode": {
    "data": "./fill.json",
    "uploadToIpfs": false,
    "deleteDownloadsAfterCreation": true,
    "deleteLargeAfterCreation": true,
    "objPath": "./tmp/obj",
    "limit": 2,
    "offest": 0
  },
  "puppetSize": 1024,
  "thumbnail": {
    "path": "thumbs",
    "image": {
      "formats": [{
        "type": "jpeg",
        "options": {
          "quality": 60
        }
      }],
      "sizes": [128, 256, 512]
    },
    "video": {
      "sizes": [128, 256, 512]
    }
  },
  "ignoreObjects": [],
  "onlyObjects": []
}