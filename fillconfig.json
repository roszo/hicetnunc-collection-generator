{
  "cloudFlareUrl": "https://cloudflare-ipfs.com/ipfs/",
  "downloadPath": "./tmp/downloads",
  "largeImagePath": "./tmp/large-images",
  "distPath": "./tmp",
  "fillMode": {
    "data": "./fill.json",
    "uploadToIpfs": false,
    "deleteDownloadsAfterCreation": false,
    "deleteLargeAfterCreation": false,
    "objPath": "./tmp/obj",
    "limit": 1000,
    "offest": 40000
  },
  "puppetSize": 1024,
  "thumbnail": {
    "path": "thumbs",
    "maxGifSizeKb": 300,
    "image": {
      "formats": [{
        "type": "jpeg",
        "options": {
          "quality": 60
        }
      }],
      "sizes": [128, 512, 1024]
    },
    "video": {
      "maxLengthSeconds": 15,
      "formats": [{
        "type": "h264"
      }],
      "sizes": [256, 512]
    }
  },
  "ignoreObjects": [],
  "onlyObjects": []
}